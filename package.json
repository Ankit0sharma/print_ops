    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrations:run": "ts-node -r tsconfig-paths/register src/main.ts migrations",
    "migrations:create": "ts-node -r tsconfig-paths/register src/main.ts migrations create"
  },
